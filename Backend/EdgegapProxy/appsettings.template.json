{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=YOUR_RENDER_POSTGRES_HOST;Database=wos_accounts;Username=YOUR_USERNAME;Password=YOUR_PASSWORD;SslMode=Require"
  },
  "Jwt": {
    "Secret": "GENERATE_64_CHAR_RANDOM_SECRET_SEE_DEPLOYMENT_CONFIG_MD",
    "Issuer": "wos-game-server",
    "Audience": "wos-game-client",
    "ExpirationDays": 7
  },
  "Edgegap": {
    "ApiBaseUrl": "https://api.edgegap.com/v1",
    "HealthCheckPort": 8080,
    "HealthCheckTimeout": 5,
    "RequiredTags": ["production"]
  }
}
